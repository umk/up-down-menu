{
  "name": "up-down-menu",
  "version": "0.1.0",
  "description": "A console menu",
  "author": "Uladzislau Maslakou <umaslakou@gmail.com>",
  "homepage": "https://github.com/umk/up-down-menu#readme",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/mjs/index.d.ts",
  "scripts": {
    "build": "rimraf ./dist && run-s build:*",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:mjs": "tsc -p tsconfig.mjs.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.{ts,js}\" --write",
    "fix:lint": "eslint src --ext .ts,.js --fix",
    "test": "run-s test:*",
    "test:prettier": "prettier \"src/**/*.{ts,js}\" --list-different",
    "test:lint": "eslint src --ext .ts,.tsx,.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/umk/up-down-menu.git"
  },
  "bugs": {
    "url": "https://github.com/umk/up-down-menu/issues"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "keywords": [
    "arrow",
    "cli",
    "command",
    "console",
    "interactive",
    "interface",
    "key",
    "keyboard",
    "line",
    "menu",
    "navigation",
    "prompt",
    "system",
    "terminal",
    "tool",
    "ui"
  ]
}
